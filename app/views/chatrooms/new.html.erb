<div class='container'>
	<div class='row'>
		<%= form_for @room do |f| %>
			<div class='form-group'>
				<div class='input-group'>
					<label>主題<%= f.text_field :title, class: 'form-control', required: true %></label>
				</div>
			</div>
			<div class='form-group'>
				<div class='input-group'>
					<%= f.submit %>
				</div>
			</div>
		<% end %>
	</div>
</div>
